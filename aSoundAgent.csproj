<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Title>aSoundAgent</Title>
        <Authors>nikozdev;pangratt;</Authors>
        <Description>self-made soundpad clone</Description>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/nikozdev/aSoundAgent</RepositoryUrl>
        <RootNamespace>nNikozdev</RootNamespace>
        <OutputType>Exe</OutputType>
        <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
</Project>
